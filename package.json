{
	"name": "@lasuillard/raindrop-client",
	"version": "0.1.1",
	"description": "Axios client built with OpenAPI generator for Raindrop.io.",
	"keywords": [
		"raindrop.io",
		"axios",
		"openapi"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/lasuillard/raindrop-client.git"
	},
	"author": {
		"name": "Yuchan Lee",
		"url": "https://github.com/lasuillard",
		"email": "lasuillard@gmail.com"
	},
	"homepage": "https://github.com/lasuillard/raindrop-client",
	"main": "./dist/index.cjs.js",
	"module": "./dist/index.es.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"generate": "openapi --input openapi.yaml --client axios --output ./src/generated --name Client",
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"test": "VITEST_ENV=unit vitest --coverage --run src",
		"test:watch": "VITEST_ENV=unit vitest --silent --coverage --watch --ui src",
		"e2e": "VITEST_ENV=e2e vitest --coverage --run e2e",
		"e2e:watch": "VITEST_ENV=e2e vitest --silent --coverage --watch --ui e2e"
	},
	"devDependencies": {
		"@types/node": "^20.9.4",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"@vitest/coverage-v8": "^0.34.6",
		"@vitest/ui": "^0.34.6",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-jsdoc": "^46.8.2",
		"openapi-typescript-codegen": "^0.25.0",
		"prettier": "^3.0.3",
		"tslib": "^2.6.2",
		"typescript": "^5.3.2",
		"vite": "^5.0.2",
		"vite-plugin-dts": "^3.6.3",
		"vitest": "^0.34.6"
	},
	"dependencies": {
		"axios": "^1.6.2",
		"form-data": "^4.0.0"
	}
}
