{
	"root": true,
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier",
		"plugin:jsdoc/recommended-typescript"
	],
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint", "jsdoc"],
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2020
	}
}
